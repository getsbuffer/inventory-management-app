<?xml version="1.0" encoding="utf-8" ?>
<Shell xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       xmlns:views="clr-namespace:IM.MAUI.Views"
       x:Class="IM.MAUI.AppShell">

    <ShellContent Title="Main Page" ContentTemplate="{DataTemplate views:MainPage}" />
    <ShellContent Title="Inventory Management" ContentTemplate="{DataTemplate views:InventoryManagementPage}" Route="InventoryManagementPage" />
    <ShellContent Title="Shop" ContentTemplate="{DataTemplate views:ShopPage}" Route="ShopPage" />
    <ShellContent Title="Cart" ContentTemplate="{DataTemplate views:CartPage}" Route="CartPage" />
    <ShellContent Title="Subscription" ContentTemplate="{DataTemplate views:SubscriptionPage}" Route="SubscriptionPage" />

</Shell>
